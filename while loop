# condition based loop

1.
password = ""
while password != "kebab":
  password = input('Please enter a password: ')
  print ("correct password accepted")

2. 
answer = "N"
print("Are we there yet?")
while answer != "Y":
  answer = str(input("Please respond, y or n: ").upper())
  print("Yay! We are there!!")

3.
print("Press 1 for  choice or 2 to exit")
StayInLoop = 1
while StayInLoop != 2:
  StayInLoop = int(input("Please respond, 1 or 2: "))
print("Exited")

4.
loopCount = 1
while loopCount < 10:
  loopCount = loopCount + 1
  print("Loop", loopCount)
print("End")

5.
username = ""
while not username:
    username = str(input("Enter username: "))
print("username is accepted")
print("hello" ,username,)

6. 
import random
number=random.randint(1,10)
userGuess=int(input("Guess the number between 1-10"))
if number < userGuess:
    guess=int(input("Guess to High, try again: "))
else:
    guess=int(input("Guess to Low, try again: "))
print("Yay! You got it correct!!!")

